#if no value, only then is it default value
def calculate(num1, num2 =4):
    res = num1*num2
    print(res)
calculate(5)

#strings can be iterated 
var = "Mana"*2*3
print(var)

#append function
sampleList = ["Mana", "Larry"]
sampleList.append("Scott")
print(sampleList)

#some fun iteration
var = "Taylor Swift"
#print(var[2::-1])
print(var[2::4])
