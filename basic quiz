#if no value, only then is it default value
def calculate(num1, num2 =4):
    res = num1*num2
    print(res)
calculate(5)

#strings can be iterated 
var = "Llama"*2*3
print(var)

#append function
sampleList = ["Llama", "Larry"]
sampleList.append("Scott")
print(sampleList)

#some fun iteration
var = "Taylor Plank"
#print(var[2::-1])
print(var[2::4])

#another iteration (
str = "synchronicity"
print(str[0:2])

#else block executes after successful execution
for i in range(1,5):
    print(i)
else:
    print("this is else block statement")
